{
  "transform": {
    "^.+\\.(t|j)s?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/) (test|spec))\\.(js?|ts?)$",
  "moduleFileExtensions": ["ts", "js"],
  "verbose": true,
  "coverageProvider": "v8",
  "collectCoverage": false,
  "coverageReporters": ["clover", "json", "lcov", "text"],
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^@/(.*)$": "./src/$1"
  }
}
